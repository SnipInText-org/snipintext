{
  "name": "snipintext",
  "version": "1.0",
  "description": "snippeter for text areas",
  "manifest_version": 2,
  "permissions": [
    "storage",
    "tabs",
    "file:///C:/projector/google_extensions/snipintext/test.html"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "options_page": "options.html",
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16" : "./images/icon.png"
    }
  },
  "content_scripts": [
    {
      "matches": "file:///C:/projector/google_extensions/snipintext/test.html",
      "js": ["content.js","jquery-3.3.1.min.js"]
    }
  ],
  "icons": {
      "16": "./images/icon.png",
      "32": "./images/icon.png",
      "48": "./images/icon.png",
      "128": "./images/icon.png"
    }
}
